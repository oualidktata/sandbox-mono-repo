{
  "npmScope": "pwc",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "settings-manage-settings": {
      "tags": ["scope:settings", "type:feature"]
    },
    "settings-shell-settings": {
      "tags": ["scope:settings", "type:shell"]
    },
    "shared-layout": {
      "tags": ["scope:layout", "type:feature"]
    },
    "shared-material": {
      "tags": ["scope:shared", "type:ui"]
    },
    "shared-ui": {
      "tags": ["scope:shared", "type:ui"]
    },
    "shared-ui-e2e": {
      "tags": [],
      "implicitDependencies": ["shared-ui"]
    },
    "user-console": {
      "tags": ["scope:user", "type:app"]
    },
    "user-console-e2e": {
      "tags": [],
      "implicitDependencies": ["user-console"]
    },
    "users-dashboard": {
      "tags": ["scope:dashboard", "type:feature"]
    },
    "users-details": {
      "tags": ["scope:users", "type:feature"]
    },
    "users-domain": {
      "tags": ["scope:users", "type:domain"]
    },
    "users-edit-user": {
      "tags": ["scope:users", "type:feature"]
    },
    "users-list": {
      "tags": ["scope:users", "type:feature"]
    },
    "users-manage-users": {
      "tags": ["scope:users", "feature:feature"]
    },
    "users-shell": {
      "tags": ["scope:users", "feature:shell"]
    }
  }
}
